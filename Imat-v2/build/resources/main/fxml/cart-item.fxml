<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.image.Image?>
<fx:root stylesheets="@../css/task.css" styleClass="selected" prefHeight="105.0" prefWidth="240.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane styleClass="selected" layoutX="0.0" layoutY="0" prefHeight="15.0" prefWidth="15.0">
        <children>
            <ImageView fx:id="deleteImage" fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true" >
                <image>
                    <Image url="@../images/trash.png"/>
                </image>
            </ImageView>
        </children>
    </StackPane>
    <Label fx:id="productNameLabel" layoutX="14.0" layoutY="18.0" text="Produkt: ">
      <font>
         <Font size="16.0" />
      </font></Label>
    <Label fx:id="productQuantityLabel" layoutX="14.0" layoutY="48.0" text="Antal: ">
      <font>
         <Font size="16.0" />
      </font></Label>
    <Label fx:id="productPriceLabel" layoutX="14.0" layoutY="78.0" text="Pris: ">
      <font>
         <Font size="16.0" />
      </font></Label>
   <StackPane styleClass="selected" layoutX="130.0" layoutY="9.0" prefHeight="100.0" prefWidth="100.0">
      <children>
         <ImageView fx:id="productImage" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
      </children>
   </StackPane>
</fx:root>
